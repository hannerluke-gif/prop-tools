{
  "name": "prop_tools",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "sass:build": "sass static/scss:static/css --style=compressed --load-path=node_modules",
    "sass:watch": "sass --watch static/scss:static/css --no-source-map --load-path=node_modules",
    "build": "sass --load-path=node_modules --quiet-deps static/scss/main.scss static/css/main.css --no-source-map",
    "watch": "sass --load-path=node_modules --quiet-deps --watch static/scss/main.scss static/css/main.css --no-source-map",
    "heroku-postbuild": "npm run build"
  },
  "devDependencies": {
    "bootstrap": "^5.3.3",
    "sass": "^1.92.1"
  }
}
